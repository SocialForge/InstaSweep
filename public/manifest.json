{
    "name": "InstaSweep",
    "version": "1.0.0",
    "description": "Follower Manager For Instagram",
    "manifest_version": 3,
    "author": "SocialForge",
    "permissions": ["activeTab", "scripting", "tabs"],
    "action": {
        "default_icon": "assets/favicon/favicon-48_inactive.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "assets/favicon/favicon-16.png",
        "32": "assets/favicon/favicon-32.png",
        "48": "assets/favicon/favicon-48.png",
        "128": "assets/favicon/favicon-128.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+I",
                "mac": "Command+I"
            }
        }
    }
}
